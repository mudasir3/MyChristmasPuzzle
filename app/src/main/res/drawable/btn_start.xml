<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="10dp" />
            <gradient android:angle="270" android:endColor="#666" android:startColor="#999" />
            <stroke android:width="1dp" android:color="#484444" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="10dp" />
            <gradient android:angle="270" android:endColor="#f9f65d" android:startColor="#fce410" />
            <stroke android:width="1dp" android:color="#535252" />
        </shape>
    </item>

</selector>